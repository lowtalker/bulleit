<?xml version="1.0" encoding="UTF-8"?>
<!-- test config file -->
<proxyConfig>
    <destinations>
        <destination>
            <name>atomizer</name>
            <destinationUrl>localhost</destinationUrl>
            <destinationPort>8181</destinationPort>
            <prefix>/atomizer</prefix>
            <asyncSupport>true</asyncSupport>
            <scheme>https</scheme>
        </destination>                         
    </destinations>
    <maxThreads>10</maxThreads>
    <maxConnections>10</maxConnections>
    <idleTimeout>50000</idleTimeout>
    <timeout>50000</timeout>
    <requestBufferSize>8192</requestBufferSize>
    <responseBufferSize>8192</responseBufferSize>
    <hostURL>localhost</hostURL>
    <hostPort>80</hostPort>
    <secureHostPort>443</secureHostPort>
</proxyConfig>
