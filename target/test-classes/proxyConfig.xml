<?xml version="1.0" encoding="UTF-8"?>
<!-- test config file -->
<proxyConfig>
    <destinations>
        <destination>
            <name>atom</name>
            <destinationUrl>atom.io</destinationUrl>
            <destinationPort>443</destinationPort>
            <prefix>/atom</prefix>
            <asyncSupport>true</asyncSupport>
            <scheme>https</scheme>
        </destination>
        <destination>
            <name>twitter</name>
            <destinationUrl>platform.twitter.com</destinationUrl>
            <destinationPort>443</destinationPort>
            <prefix>/twitter</prefix>
            <asyncSupport>true</asyncSupport>
            <scheme>https</scheme>
        </destination>                   
    </destinations>
    <maxThreads>10</maxThreads>
    <maxConnections>10</maxConnections>
    <idleTimeout>50000</idleTimeout>
    <timeout>50000</timeout>
    <requestBufferSize>8192</requestBufferSize>
    <responseBufferSize>8192</responseBufferSize>
    <hostURL>localhost</hostURL>
    <hostPort>8080</hostPort>
    <secureHostPort>443</secureHostPort>
</proxyConfig>
